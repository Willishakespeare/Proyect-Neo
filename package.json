{
  "name": "proyect-neo",
  "version": "0.1.2",
  "description": "n/a",
  "main": "main.js",
  "scripts": {
    "start": "electron main.js",
    "mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "win": "electron-packager . proyect-neo --overwrite --asar=false --platform=win32 --arch=x64 --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Proyect Neo\"",
    "linux": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icons/png/1024x1024.png --prune=true --out=release-builds"
  },
